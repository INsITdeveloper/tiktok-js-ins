  "/toanime": {
      "get": {
        "tags": ["ToAnime"],
        "summary": "Convert video to anime",
        "description": "Converts a video to anime using Skizo API",
        "parameters": [
          {
            "name": "apikey",
            "in": "query",
            "description": "Your Skizo API key",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "url",
            "in": "query",
            "description": "URL of the video to be converted",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "description": "Status of the conversion process"
                    },
                    "result": {
                      "type": "string",
                      "description": "URL of the converted anime video"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "description": "Description of the error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "requestBodies": {
      "onlyUrl": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/onlyUrl"
            },
            "examples": {
              "web url": { "$ref": "#/components/examples/weburl" },
              "mobile url": { "$ref": "#/components/examples/mobileurl" }
            }
          }
        }
      }
    },
    "examples": {
      "weburl": {
        "description": "tiktok video url from website",
        "value": {
          "url": "https://vt.tiktok.com/ZSFs1FYbx/"
        }
      },
      "mobileurl": {
        "description": "url video tiktok dari aplikasi seluler",
        "value": {
          "url": "https://vt.tiktok.com/ZSFs1YCUR/"
        }
      }
    },
    "parameters": {
      "onlyUrl": {
        "name": "url",
        "required": true,
        "description": "tiktok video url",
        "in": "query",
        "schema": {
          "type": "string",
          "nullable": false
        },
        "examples": {
          "web url": {
            "description": "tiktok url from desktop web",
            "value": "https://vt.tiktok.com/ZSFs1FYbx/"
          },
          "mobile url": {
            "description": "tiktok url from mobile",
            "value": "https://vt.tiktok.com/ZSFs1YCUR/"
          }
        }
      }
    },
    "schemas": {
      "onlyUrl": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "required": true,
            "default": "https://vt.tiktok.com/ZSFs1N2cx/"
          }
        }
      }
